                                </li>
                            </ul>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
<!-- end of header--->

<% if (req.session.houses.length > 0) { %>

<section class="slider">
    <div class="container">
        <div class="addRooms">
            <h2>Add House</h2>
            <form action="/x/houseadd" method="post">
                <div class="field">
                    <input type="text" id="fieldUser" name="name" class="input" required pattern=.*\S.* />
                    <label for="fieldUser" class="label">House Name</label>
                </div>
                <div class="field">
                    <!--<input type="text" id="fieldPassword"  name="image" class="input" required pattern=.*\S.* />-->
                    <!--<label for="fieldPassword" class="label">House Image</label>-->

                    <select name="image" class="input">
                        <option value="/images/GivenHouses/Apartment.png">Apartment</option>
                        <option value="/images/GivenHouses/Beach House.png">Beach House</option>
                        <option value="/images/GivenHouses/Cabin in the Woods.png">Cabin in the Woods</option>
                        <option value="/images/GivenHouses/RV.png">RV</option>
                    </select>
                </div>
                <div class="btn btn-primary">
                    <input type="submit" name="Submit" style="background: transparent;">
                </div>
            </form>
        </div>
        <div class="my-slider">
            <div class="prev-nav"></div>
            <div class="next-nav"></div>
            <div class="main-slider ">
                <% _.each(req.session.houses, function (corndog) { %>

                <div class="slide-item" >
                    <img src=""  alt="Slider Saul" style="background: transparent"/>

                    <div class="item-caption">
                        <div id="myImage">
                            <img src="<%= corndog.image %>" alt="SliderItem" />
                        </div>

                        <h2><%= corndog.name%></h2>
                        <a href="/x/housedelete?id=<%= corndog.id %>">Destroy</a>|
                        <a href="/x/roomreturnAll?id=<%= corndog.id %>">Rooms</a>
                    </div>
                </div>
                <% }) %>
            </div>
        </div>
    </div>
</section>

<% } else { %>

<section>

    <div class="container">
            <h2>Add House</h2>
            <form action="/x/houseadd" method="post">
                <div class="field">
                    <input type="text" id="fieldUser" name="name" class="input" required pattern=.*\S.* />
                    <label for="fieldUser" class="label">House Name</label>
                </div>
                <div class="field">
                    <!--<input type="text" id="fieldPassword"  name="image" class="input" required pattern=.*\S.* />-->
                    <!--<label for="fieldPassword" class="label">House Image</label>-->

                    <select name="image" class="input">
                        <option value="/images/GivenHouses/Apartment.png">Apartment</option>
                        <option value="/images/GivenHouses/Beach House.png">Beach House</option>
                        <option value="/images/GivenHouses/Cabin in the Woods.png">Cabin in the Woods</option>
                        <option value="/images/GivenHouses/RV.png">RV</option>
                    </select>
                </div>
                <div class="btn btn-primary">
                    <input type="submit" name="Submit" style="background: transparent;">
                </div>
            </form>
    </div>
</section>
<% } %>



